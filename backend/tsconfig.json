{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "../target",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "typeRoots": ["./node_modules/@types"],
   "types": ["node", "express"],
  "exclude": ["node_modules"]
}
